# Set the command-line arguments to pass to the server.
ARGS="\
  -config.file=<%= @conf_file %> \
  -alertmanager.notification-queue-capacity=<%= @defaults['alertmanager']['notification_queue_capacity'] %> \
  -alertmanager.timeout=<%= @defaults['alertmanager']['timeout'] %> \
  -log.level=<%= @defaults['log']['level'] %> \
  -query.max-concurrency=<%= @defaults['query']['max_concurrency'] %> \
  -query.lookback-delta=<%= @defaults['query']['lookback'] %> \
  -query.timeout=<%= @defaults['query']['timeout'] %> \
  -web.listen-address=<%= @listen_addr %> \
  -web.external-url=<%= @defaults['web']['external_url'] %> \
  -web.local-assets=<%= @defaults['web']['local_assets'] %> \
  -web.user-assets=<%= @defaults['web']['user_assets'] %> \
  -web.console.libraries=<%= @defaults['web']['console']['libraries'] %> \ 
"

